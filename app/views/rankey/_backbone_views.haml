%script{ type: "text/template", id: "rankeyView" }
  :plain
    %div
      .loading
      #content

%script{ type: "text/template", id: "keysListView" }<
  :plain
    %div
      %h3 Keywords
      %table{ cellspacing: 0 }
        %thead
          %tr
            %th name
            %th google
            %th yahoo
            %th bing
        %tbody.keysList
    

%script{ type: "text/template", id: "keyRow" }<
  :plain
    %td \#{name}
    :for pos in positions
      %td
        %div= pos.pos

%script{ type: "text/template", id: "siteView" }<
  :plain
    .site
      %h2= name
      .cont
        %a{ href: link, target: "_blank" } 
          %img.sshot{ src: image }
        .siteChart
        #tableBtns
          .pos 
            = keys_pos_count
             positioneds 
          .totals
            = keys_count
             totals
                          
        .keysListView
      .foot &nbsp;    

%script{ type: "text/template", id: "siteRow" }<
  :plain
    %td 
      %img.favicon{ src: favicon }
      [sparkline] \#{name}
    %td= keys_pos_count
    %td= keys_count

%script{ type: "text/template", id: "sitesView" }<
  :plain
    %div
      #search
        %input{ name: "key", type: "search" }
        %img.icon{ src: "/images/icons/search.png" }
      .clear
      %table{ cellspacing: 0 }
        %thead
          %tr
            %th name
            %th positioneds
            %th totals
        %tbody.sitesList