%script{ type: "text/template", id: "keysListView" }<
  :plain
    %div
      %h3 Keywords
      %table{ cellspacing: 0 }
        %thead
          %tr
            %th name
            %th google
            %th yahoo
            %th bing
        %tbody.keysList
    

%script{ type: "text/template", id: "keyRow" }<
  :plain
    %td \#{name}
    :for pos in positions
      %td
        %div
          :if pos.attributes.pos != undefined
            = pos.attributes.pos
          :if pos.attributes.pos == undefined
            ?
