%script{ type: "text/template", id: "keysListView" }<
  :plain
    %div
      %h3 Keywords
      %a.btn.edit Edit
      .clear
      #keys_src
      %table{ cellspacing: 0 }
        %thead
          %tr
            %th name
            %th google
            %th yahoo
            %th bing
        %tbody.keysList
    

%script{ type: "text/template", id: "keyRow" }<
  :plain
    %td \#{name}
    :for pos in positions
      %td
        :if pos.attributes.pos != undefined
          %div.pos{ data-link: pos.attributes.link }
            = pos.attributes.pos
        :if pos.attributes.pos == undefined
          :if !cur_user.attributes.optimist
            %div  
              100+
